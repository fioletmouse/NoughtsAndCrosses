@using NoughtsAndCrosses.Models
@model List<GameInfoView>
@{
    if(Model.Count > 0)
    {
        foreach (var summary in Model)
        {
            //привести див в порядок, а то все идентификаторы открыты
            <div style="border:solid 1px; padding:2px; margin:5px" class="quote" onclick="getajax(this)" id="@summary.Id">
                @summary.Result
                 @summary.PlayerName

                @summary.CreatedOn
                @summary.MoveCount<br />
                <div class="MoveInformation"></div>
            </div>
        }
    }
    else
    {
        <p>Пусто</p>
    }
}
